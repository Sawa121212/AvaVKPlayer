<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:regions="clr-namespace:Common.Core.Regions;assembly=Common.Core"
        prism:ViewModelLocator.AutoWireViewModel="True"
        x:Class="AvaVKPlayer.Views.ShellView"
        WindowStartupLocation="CenterScreen"
        Icon="/Assets/AvaVKPlayer.ico"
        Title="AvaVKPlayer"
        MinWidth="650" MinHeight="500"
        Width="900" Height="700">

    <Panel>
        <!-- Region -->
        <ContentControl prism:RegionManager.RegionName="{x:Static regions:RegionNameService.ShellRegionName}" />
    </Panel>
</Window>